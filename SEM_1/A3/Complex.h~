class Complex{
 public:
  Complex();
  Complex(double, double=0);
  ~Complex();
  setReal(double);
  setImag(double);
  getReal(double);
  getImag(double);
  add(Complex);
  subtract(Complex);
  multiply(Complex);
  print();

 private:
  double real;
  double imag;
  

};
