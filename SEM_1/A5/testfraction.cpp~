#include <iostream>
#include "fraction.h"

using namespace std;

int main(void)
{
        Fraction a, b, c;

	cin >> a >> b >>c;

	cout << endl;
	cout << a << endl;
	cout << b << endl; 
	cout << c << endl;

	cout << a * b << endl;
	cout << a/b << endl;


	cout << a << "\n" << b << "\n";
	return 0;
}
