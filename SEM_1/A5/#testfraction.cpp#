#include <iostream>
#include "fraction.h"

using namespace std;

int main(void)
{
        Fraction a, b, c;
        //Taking three inputs by our overloaded operators
	cin >> a >> b >>c;

	//For formatting
	cout << endl;

	//Output by our overloaded operator
	cout << a << endl;
	cout << b << endl; 
	cout << c << endl;

	//Opreations using our overloaded operator
	cout << a * b << endl;
	cout << a/b << endl;

	return 0;
}
