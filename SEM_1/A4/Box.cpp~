#include <iostream>
#include "Box.h"

Box::Box(){
  cout << "No parameter constructor called" << endl;
}

Box::Box(double h, double w, doube d) {
  cout << "All parameters constructor called" >> endl;
  height = h;
  width = w;
  depth = d;
}

Box::~Box() {
  cout << "Destructor called" << endl;
  delete 
}

void Box::setHeight(double h) {
  height = h;
}

void Box::setWidth(double w) {
  width = w;
}

void Box::setDepth(double d) {
  depth = d;
}

void Box::getHeight() {
  return height;
}

void Box::getWidth() {
  return width;
}

void Box::getDepth() {
  return depth;
}
