#include <iostream>

class Box{
 public:
  Box();
  Box(h, w, d);

  //Setters
  void setHeight(double);
  void setWidth(double);
  void setDepth(double);

  //Getters
  double getHeight();
  double getWidth();
  double getDepth();
  
 private:
  double height;
  double width;
  double depth;
};


