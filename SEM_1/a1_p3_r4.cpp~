#include <iosream>
#include <string>
using namespace std;

int main() {
  return 0;
}

int mycount(int a, int b) {
  return a-b;
}

int mycount(string s, char c){
  int count = 0;
  for (unsigned i = 0; i < s.length(); i++){
    if (s[i] == c)
      count++;
  }
  if (count == 0)
    return -1;
  
  return count;
}
